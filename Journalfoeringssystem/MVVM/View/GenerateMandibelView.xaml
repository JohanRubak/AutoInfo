      <TextBox TabIndex="6" x:Name="NameWorker_Copy2" HorizontalAlignment="Left" Margin="96,521,0,0" Text="{Binding Piece3Length, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="53" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="NameWorker_MouseDoubleClick" GotKeyboardFocus="NameWorker_GotKeyboardFocus"/>
      <Label Content="Piece 2:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="31,489,0,0" FontWeight="Bold"/>
