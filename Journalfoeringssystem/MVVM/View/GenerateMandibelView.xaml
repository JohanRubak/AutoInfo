      <TextBox TabIndex="5" x:Name="SerieTextbox" HorizontalAlignment="Left" Margin="282,254,0,0" Text="{Binding SerieOfScanning1, FallbackValue='#X', TargetNullValue='#X'}" TextWrapping="Wrap" VerticalAlignment="Top" Width="21" Foreground="#FFC8C8C8" Background="#747999" GotKeyboardFocus="SerieTextbox_GotKeyboardFocus"/>
      <Label Content="Serie of scanning:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="172,248,0,0"/>

      <!--Scanning2-->
      <Label Content="Date of scanning:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="405,190,0,0"/>
      <TextBox TabIndex="4" x:Name="TypeOfScanningTextbox_Copy" HorizontalAlignment="Left" Margin="515,224,0,0" Text="{Binding TypeOfScanning2, FallbackValue=XXXX, TargetNullValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="102" Foreground="#FFC8C8C8" Background="#747999" GotKeyboardFocus="TypeOfScanningTextbox_GotKeyboardFocus"/>
      <DatePicker TabIndex="3" x:Name="DateOfScanningDP_Copy" SelectedDate="{Binding DateForScanning2}" HorizontalAlignment="Left" Margin="515,192,0,0" VerticalAlignment="Top" Width="102"/>
      <Label Content="Type of scanning:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="405,218,0,0"/>
      <TextBox TabIndex="5" x:Name="SerieTextbox_Copy" HorizontalAlignment="Left" Margin="515,254,0,0" Text="{Binding SerieOfScanning2, FallbackValue=#X, TargetNullValue=#X}" TextWrapping="Wrap" VerticalAlignment="Top" Width="21" Foreground="#FFC8C8C8" Background="#747999" GotKeyboardFocus="SerieTextbox_GotKeyboardFocus"/>
      <Label Content="Serie of scanning:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="405,248,0,0"/>
      <Label Content="Scanning 2:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="405,164,0,0" FontWeight="Bold"/>

      <!--Resection-->
      <Label Content="Resection:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="442,300,0,0" FontWeight="Bold" RenderTransformOrigin="0.615,0.591" Width="142"/>
      <Label Content="To:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="442,380,0,0"/>
      <Label Content="Distance to malleol:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="442,407,0,0" Width="121"/>
