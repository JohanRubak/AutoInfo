      <TextBox TabIndex="6" x:Name="NameWorker_Copy6" HorizontalAlignment="Left" Margin="560,493,0,0" Text="{Binding ScrewDiameter, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="55" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="NameWorker_MouseDoubleClick" GotKeyboardFocus="NameWorker_GotKeyboardFocus"/>
      <TextBox TabIndex="7" x:Name="WorkerTitel_Copy5" HorizontalAlignment="Left" Margin="560,521,0,0" Text="{Binding Direction, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="55" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="WorkerTitel_MouseDoubleClick" GotKeyboardFocus="WorkerTitel_GotKeyboardFocus"/>
      <Label Content="Total length:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="442,435,0,0" Height="26" Width="78"/>
      <TextBox TabIndex="7" x:Name="WorkerTitel_Copy6" HorizontalAlignment="Left" Margin="560,466,0,0" Text="{Binding CuttingThickness, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="55" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="WorkerTitel_MouseDoubleClick" GotKeyboardFocus="WorkerTitel_GotKeyboardFocus"/>
      <TextBox TabIndex="7" x:Name="WorkerTitel_Copy7" HorizontalAlignment="Left" Margin="560,439,0,0" Text="{Binding TotalLength, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="55" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="WorkerTitel_MouseDoubleClick" GotKeyboardFocus="WorkerTitel_GotKeyboardFocus"/>
      <Label Content="Direction:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="442,515,0,0"/>

