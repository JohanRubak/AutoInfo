      <TextBox TabIndex="7" x:Name="WorkerTitel_Copy3" HorizontalAlignment="Left" Margin="168,466,0,0" Text="{Binding Piece1PlacingOfFibula, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="106" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="WorkerTitel_MouseDoubleClick" GotKeyboardFocus="WorkerTitel_GotKeyboardFocus"/>
      <Label Content="Piece 3:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="31,517,0,0" FontWeight="Bold"/>
      <TextBox TabIndex="6" x:Name="NameWorker_Copy5" HorizontalAlignment="Left" Margin="168,493,0,0" Text="{Binding Piece2PlacingOfFibula, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="106" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="NameWorker_MouseDoubleClick" GotKeyboardFocus="NameWorker_GotKeyboardFocus"/>
      <TextBox TabIndex="7" x:Name="WorkerTitel_Copy4" HorizontalAlignment="Left" Margin="292,466,0,0" Text="{Binding Piece1PlacingOfMandibel, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="108" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="WorkerTitel_MouseDoubleClick" GotKeyboardFocus="WorkerTitel_GotKeyboardFocus"/>
      <TextBox TabIndex="6" x:Name="NameWorker_Copy4" HorizontalAlignment="Left" Margin="292,493,0,0" Text="{Binding Piece2PlacingOfMandibel, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="108" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="NameWorker_MouseDoubleClick" GotKeyboardFocus="NameWorker_GotKeyboardFocus"/>
      <TextBox TabIndex="6" x:Name="NameWorker_Copy7" HorizontalAlignment="Left" Margin="292,521,0,0" Text="{Binding Piece3PlacingOfMandibel, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="108" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="NameWorker_MouseDoubleClick" GotKeyboardFocus="NameWorker_GotKeyboardFocus"/>

      <!--Comments-->
