      <Label Content="Fibula:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="442,327,0,0"/>
      <Label Content="From:" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#ccd7ff" Margin="442,354,0,0" Height="26" Width="73"/>
      <TextBox TabIndex="7" x:Name="WorkerTitel_Copy2" HorizontalAlignment="Left" Margin="560,358,0,0" Text="{Binding ResectionFrom, FallbackValue=XXXX}" TextWrapping="Wrap" VerticalAlignment="Top" Width="55" Foreground="#FFC8C8C8" Background="#747999" MouseDoubleClick="WorkerTitel_MouseDoubleClick" GotKeyboardFocus="WorkerTitel_GotKeyboardFocus"/>
      <RadioButton Content="Right" Command="{Binding SelectedRadioButton}" CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Content}" HorizontalAlignment="Left" Margin="516,332,0,0" VerticalAlignment="Top" Foreground="#ccd7ff"/>
      <RadioButton Content="Left" Command="{Binding SelectedRadioButton}" CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Content}" HorizontalAlignment="Left" Margin="576,332,0,0" VerticalAlignment="Top" Foreground="#ccd7ff"/>
