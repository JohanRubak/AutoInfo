<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Journalfoeringssystem.MVVM.View"
             xmlns:ViewModel="clr-namespace:Journalfoeringssystem.MVVM.ViewModel" x:Class="Journalfoeringssystem.MVVM.View.GeneratePDFView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
   <UserControl.DataContext>
      <ViewModel:GeneratePDFViewModel></ViewModel:GeneratePDFViewModel>
   </UserControl.DataContext>

   <Grid>
      <TextBox HorizontalAlignment="Left" Margin="150,135,0,0" Text="{Binding CPRNumber}" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
         <Button Content="Button" HorizontalAlignment="Left" Margin="163,189,0,0" VerticalAlignment="Top" Command="{Binding SearchCommand}"/>
      <TextBox HorizontalAlignment="Left" Margin="425,209,0,0" Text="{Binding Name}" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

   </Grid>
</UserControl>
